{
	"name": "flowletdemo",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregate1"
				},
				{
					"name": "select1"
				},
				{
					"name": "input1"
				},
				{
					"name": "output1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          Id as string,",
				"          Name as string,",
				"          gender as string",
				"     ),",
				"     order: 0) ~> input1",
				"input1 aggregate(groupBy(Id,",
				"          Name,",
				"          gender),",
				"     count = count()) ~> aggregate1",
				"aggregate1 select(mapColumn(",
				"          Id,",
				"          Name,",
				"          gender",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 output() ~> output1"
			]
		}
	}
}